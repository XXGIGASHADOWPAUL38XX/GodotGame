; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Game"
run/main_scene="res://Game/Scenes/Root/root.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

ServiceSpell="*res://Game/Services/service_spell.gd"
ServiceScenes="*res://Game/Services/service_scenes.gd"
ServiceHealth="*res://Game/Services/service_health.gd"
State="*res://Game/Classes/State.gd"
Server="*res://Game/Services/Server/server.gd"
ServiceStats="*res://Game/Services/service_stats.gd"
ServiceText="*res://Game/Services/service_text.gd"
Servrpc="*res://Game/Services/Server/servrpc.gd"
ServiceAnimations="*res://Game/Services/service_animations.gd"
ServiceAnnounce="*res://Game/Services/service_announce.gd"
ServiceMovements="*res://Game/Services/service_movements.gd"
ServiceOrbs="*res://Game/Services/service_orbs.gd"
UpdateMode="*res://Game/Classes/UpdateMode.gd"
ServiceEvents="*res://Game/Services/service_events.gd"
MageOrb="*res://Game/Scenes/mage_f/orb_kind.gd"
ServiceRounds="*res://Game/Services/service_rounds.gd"
ServiceSettings="*res://Game/Services/service_settings.gd"
ServiceWindow="*res://Game/Services/service_window.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/viewport_width=1200
window/size/viewport_height=680
window/size/resizable=false

[dotnet]

project/assembly_name="Game"

[layer_names]

2d_physics/layer_1="Champions"
2d_physics/layer_2="Spells"

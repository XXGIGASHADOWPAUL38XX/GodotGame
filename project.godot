; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Game"
run/main_scene="res://Game/Scenes/Menu/menu_scene.tscn"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

ServiceSpell="*res://Game/Services/service_spell.gd"
ServiceTime="*res://Game/Services/service_time.gd"
ServiceScenes="*res://Game/Services/service_scenes.gd"
ServiceHealth="*res://Game/Services/service_health.gd"
State="*res://Game/Classes/State.gd"
Server="*res://Game/Services/Server/server.gd"
ServiceStats="*res://Game/Services/service_stats.gd"
ServiceText="*res://Game/Services/service_text.gd"
Servrpc="*res://Game/Services/Server/servrpc.gd"
ServiceAnimations="*res://Game/Services/service_animations.gd"
ServiceAnnounce="*res://Game/Services/service_announce.gd"
ServiceMovements="*res://Game/Services/service_movements.gd"
ServiceCollision="*res://Game/Services/service_collision.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/viewport_width=1200
window/size/viewport_height=680

[dotnet]

project/assembly_name="Game"

[layer_names]

2d_physics/layer_1="Champions"
2d_physics/layer_2="Spells"
